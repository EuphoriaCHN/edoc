{
  "private": true,
  "scripts": {
    "start": "node ./scripts/dev.js",
    "build": "./build.sh",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test:pro": "node ./__tests__/index.js",
    "analyze": "SET ANALYZE=1 && umi build",
    "i18n-gen": "i18next-scanner --config ./i18next-scanner.config.js --output ./ './src/**/*.{js,jsx,ts,tsx}'",
    "i18n": "yarn i18n-gen && node ./scripts/i18n.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.1.4",
    "@ant-design/pro-layout": "^6.5.0",
    "@types/webpack-env": "^1.16.0",
    "@umijs/preset-react": "1.x",
    "axios": "^0.21.1",
    "i18next": "^20.2.4",
    "js-cookie": "^2.2.1",
    "lodash-es": "^4.17.21",
    "prism-react-renderer": "^1.2.1",
    "react-i18next": "^11.8.15",
    "umi": "^3.4.21"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "^3.4.21",
    "ali-oss": "^6.15.2",
    "google-translate-open-api": "^1.3.7",
    "i18next-scanner": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
